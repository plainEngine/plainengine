newobj $1
objsetf $1 mass 0
objsetf $1 restitution 0.01
objsetf $1 friction 0.5
objsetf $1 positionable
setXYZ $2 $3 0 $1
objsetf $1 renderable
#objsetfwp $1 physical 1 shapeType=box/XShape=0.21/YShape=0.21/ZShape=0.1
objsetfwp $1 physical 1 shapeType=sphere/radius=0.21
objsetfwp $1 renderable "" textureName=bubble.png/scaleX=0.21/scaleY=0.21

objsetf $1 attractorType $6
objsetf $1 attractingRadius $4
objsetf $1 attractingImpulseCoefficient $5
objsetf $1 attractor
